{
    "name": "Translator",
    "description": "Translate text from one language to another",
    "version": "0.2",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "tooltip.css"
            ]
        }
    ]
}